{
  "name": "mcp-to-typescript-codegen",
  "version": "0.1.0",
  "description": "Autogenerate Typescript types from MCP servers to be consumed by AI SDK (or any other framework)",
  "license": "MIT",
  "bin": {
    "mcp-to-typescript-codegen": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bunup",
    "dev": "bunup --watch",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/bun": "^1.3.3",
    "bunup": "^0.16.10",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.24.3",
    "json-schema-to-zod": "^2.7.0",
    "ts-morph": "^27.0.2",
    "zod": "^4.1.13"
  }
}
